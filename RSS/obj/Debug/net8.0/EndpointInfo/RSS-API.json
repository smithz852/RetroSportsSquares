{
  "openapi": "3.0.1",
  "info": {
    "title": "RSS-API",
    "version": "1.0"
  },
  "paths": {
    "/AvailableGames/GetAvailableGames": {
      "get": {
        "tags": [
          "AvailableGames"
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/AvailableGames/CreateGame": {
      "post": {
        "tags": [
          "AvailableGames"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/CreateGameDTO"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/CreateGameDTO"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/CreateGameDTO"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "CreateGameDTO": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "nullable": true
          },
          "gameType": {
            "type": "string",
            "nullable": true
          },
          "status": {
            "type": "string",
            "nullable": true
          },
          "playerCount": {
            "type": "number",
            "format": "double"
          }
        },
        "additionalProperties": false
      }
    }
  }
}